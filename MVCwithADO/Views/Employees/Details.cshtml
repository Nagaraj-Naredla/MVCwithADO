
@model MVCwithADO.Models.Employees
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <h1>Employee Information</h1>
        <div>
            Employee ID is:@Model.EmpID <br />
            Employee Name is:@Model.EmpName <br />
            Employee Salary is:@Model.Salary <br />
            Employee Job is:@Model.Job <br />
            Employee Department ID is:@Model.DeptID<br />
        </div>
        <div>
            <a href="/"class="btn btn-outline-info">Back to Index</a>
        </div>
    </div>
</body>
</html>
